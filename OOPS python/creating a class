class Cuboid:
    def __init__(self,l,b,h):
        print(id(self))
        self.length=l
        self.breadth=b
        self.height=h
    def lateral_area(self):
        return 2*(self.length*self.breadth+self.breadth*self.height+self.length*self.height)
    def volume(self):
        return self.length*self.breadth*self.height
c1=Cuboid(10,2,2)
#print(c1.volume())

c2=Cuboid(10,10,10)
#print(c2.lateral_area())
print(id(c1))
